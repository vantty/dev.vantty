{
  "name": "vantty-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "server": "nodemon .",
    "dev": "concurrently \"npm run server\" \"npm start --prefix ../vantty\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Vantty Team",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.5.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.1.0",
    "elasticsearch": "^16.6.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.11",
    "express-validator": "^5.3.1",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "stripe": "^7.15.0",
    "twilio": "^3.41.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "jest": "^25.2.7",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "12.x"
  }
